{
  "name": "pharmapro",
  "version": "0.0.1",
  "main": "main.js",
  "description": "Pharma Pro",
  "homepage": "http://care9.com",
  "author": {
    "name": "ShivaGanesh Bellamkoknda",
    "email": "appsupport@care9.com"
  },
  "keywords": [
    "Pharma Soft",
    "Care9",
    "Izatech",
    "Pharmacy Software"
  ],
  "build": {
    "publish": [{
        "provider": "github",
        "owner": "arkyminds-vasikarans",
        "repo": "pharma-pro-test"
    }]},
  "scripts": {
    "build": "npm run postinstall:electron && npm run electron:tsc && ng build --optimization=false",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "electron:tsc": "tsc main.ts",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:tsc && electron ./ --serve",
    "electron:local": "npm run build:prod && electron .",
    "electron:linux": "npm run build:prod && npx electron-builder build --linux",
    "electron:windows": "npm run build:prod && npx electron-builder build --windows",
    "electron:mac": "npm run build:prod && npx electron-builder build --mac",
    "start": "npm run postinstall:electron && npm-run-all -p ng:serve electron:serve",
    "ng:serve": "ng serve",
    "ng:serve:web": "npm run postinstall:web && npm run electron:tsc  && ng serve -o",
    "postinstall": "npm run postinstall:electron && npx electron-builder install-app-deps",
    "postinstall:web": "node postinstall-web",
    "postinstall:electron": "node postinstall"
  },
  "private": true,
  "dependencies": {
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@toverux/ngx-sweetalert2": "^4.0.0",
    "bootstrap": "^4.3.1",
    "electron-log": "^3.0.1",
    "electron-updater": "^4.0.6",
    "jquery": "^3.3.1",
    "ngx-bootstrap": "^3.2.0",
    "ngx-sweetalert2": "^0.2.7",
    "popper": "^1.0.1",
    "popper.js": "^1.14.7",
    "rxjs-compat": "^6.4.0",
    "sqlite3": "4.0.6",
    "sweetalert2": "^7.33.1",
    "typeorm": "^0.2.7"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.7.2",
    "@angular-devkit/build-angular": "^0.6.8",
    "@angular-devkit/core": "^0.7.2",
    "@angular/animations": "^6.1.1",
    "@angular/cdk": "^6.4.2",
    "@angular/cli": "~6.0.3",
    "@angular/common": "^6.1.1",
    "@angular/compiler": "^6.1.1",
    "@angular/compiler-cli": "^6.1.1",
    "@angular/core": "^6.1.1",
    "@angular/forms": "^6.1.1",
    "@angular/http": "^6.1.1",
    "@angular/language-service": "^6.1.1",
    "@angular/material": "^6.4.2",
    "@angular/platform-browser": "^6.1.1",
    "@angular/platform-browser-dynamic": "^6.1.1",
    "@angular/router": "^6.1.1",
    "@types/node": "~8.9.4",
    "angular-cli-builders": "^2.1.2",
    "codelyzer": "~4.2.1",
    "commonjs": "0.0.1",
    "copy-webpack-plugin": "^4.5.2",
    "core-js": "^2.5.4",
    "electron": "^2.0.18",
    "electron-builder": "^20.27.0",
    "electron-reload": "^1.2.5",
    "fs": "0.0.1-security",
    "hammerjs": "^2.0.8",
    "npm-run-all": "^4.1.3",
    "npx": "^10.2.0",
    "rxjs": "6.0.0",
    "stream": "0.0.2",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "2.8",
    "wait-on": "2.1.0",
    "zone.js": "^0.8.26"
  }
}
